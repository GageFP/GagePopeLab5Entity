@model GagePopeLab5.Models.Weapon

@{
    ViewData["Title"] = "Details";
}
<link rel="stylesheet" href="~/css/site.css" type="text/css" />
<h1>Details</h1>

<div>
    <h4>Weapon</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.weaponName)
        </dd>
        <dt class="col-sm-2">
            Damage
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.weaponDamage)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.weaponName">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
